{
  "format_version": "1.13.0",
  "code:place_scattered": {
    "description": {
      "identifier": "badger:feature_buff_speed_scatter_drylands_small"
    },
    "places_feature": "badger:feature_buff_speed_weighted_drylands",
    "iterations": "math.random(4,7)",
    "scatter_chance": "return 100 * (1 - query.is_in_village(variable.originx, variable.originz));",
    "project_input_to_floor": false,
    "x": {
      "extent": [-4, 4],
      "distribution": "gaussian"
    },
    "z": {
      "extent": [-4, 4],
      "distribution": "gaussian"
    },
    "y": {
      "extent": [-2, 2],
      "distribution": "gaussian"
    }
  }
}
